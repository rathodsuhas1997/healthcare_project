{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-healthcare-adf"
		},
		"LS_ADB_NOTEBOOK_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_ADB_NOTEBOOK'"
		},
		"LS_HEALTHCARE_SINK_ADLS_JSON_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_HEALTHCARE_SINK_ADLS_JSON'"
		},
		"LS_HEALTHCARE_SQL_CONFIG_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_HEALTHCARE_SQL_CONFIG'"
		},
		"LS_ADB_NOTEBOOK_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0217-181607-x0d89n03"
		},
		"LS_HEALTHCARE_API_SOURCE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://r4.smarthealthit.org/"
		},
		"LS_HEALTHCARE_SINK_ADLS_JSON_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devhealthcarestorage12.dfs.core.windows.net/"
		},
		"LS_HEALTHCARE_SQL_CONFIG_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dev-healthcare-sql-server.database.windows.net"
		},
		"LS_HEALTHCARE_SQL_CONFIG_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dev_healthcare_sql_database"
		},
		"LS_HEALTHCARE_SQL_CONFIG_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "suhas"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-healthcare-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADB_NOTEBOOK_accessToken": {
						"value": "[parameters('LS_ADB_NOTEBOOK_accessToken')]"
					},
					"LS_HEALTHCARE_SINK_ADLS_JSON_accountKey": {
						"value": "[parameters('LS_HEALTHCARE_SINK_ADLS_JSON_accountKey')]"
					},
					"LS_HEALTHCARE_SQL_CONFIG_password": {
						"value": "[parameters('LS_HEALTHCARE_SQL_CONFIG_password')]"
					},
					"LS_ADB_NOTEBOOK_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_ADB_NOTEBOOK_properties_typeProperties_existingClusterId')]"
					},
					"LS_HEALTHCARE_API_SOURCE_properties_typeProperties_url": {
						"value": "[parameters('LS_HEALTHCARE_API_SOURCE_properties_typeProperties_url')]"
					},
					"LS_HEALTHCARE_SINK_ADLS_JSON_properties_typeProperties_url": {
						"value": "[parameters('LS_HEALTHCARE_SINK_ADLS_JSON_properties_typeProperties_url')]"
					},
					"LS_HEALTHCARE_SQL_CONFIG_properties_typeProperties_server": {
						"value": "[parameters('LS_HEALTHCARE_SQL_CONFIG_properties_typeProperties_server')]"
					},
					"LS_HEALTHCARE_SQL_CONFIG_properties_typeProperties_database": {
						"value": "[parameters('LS_HEALTHCARE_SQL_CONFIG_properties_typeProperties_database')]"
					},
					"LS_HEALTHCARE_SQL_CONFIG_properties_typeProperties_userName": {
						"value": "[parameters('LS_HEALTHCARE_SQL_CONFIG_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}